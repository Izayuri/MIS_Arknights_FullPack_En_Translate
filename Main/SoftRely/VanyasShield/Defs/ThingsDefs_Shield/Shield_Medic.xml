<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <ThingDef ParentName="AK_BeltBase">
        <defName>AK_ShieldBelt_Shining</defName>
        <label>黑恶魔的庇护</label>
        <description>无论繁荣，还是灭亡。</description>
        <statBases>
            <EnergyShieldRechargeRate>0.2</EnergyShieldRechargeRate>
            <EnergyShieldEnergyMax>2</EnergyShieldEnergyMax>
        </statBases>
        <graphicData>
            <texPath>Things/ShieldBelt/Medic/ShieldBeltShining</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <comps>
            <li Class="VanyaMod.CompProperties_VanyaUniversalShieldBelt">
                <startingTicksToReset>4800</startingTicksToReset>
                <energyOnReset>0.3</energyOnReset>
                <energyLossPerDamage>0.01</energyLossPerDamage>
                <canAbsorbMeleeDamage>true</canAbsorbMeleeDamage>
                <meleeAbsorbFactor>1</meleeAbsorbFactor>
                <rangedAbsorbFactor>2</rangedAbsorbFactor>
                <canDodge>false</canDodge>
                <breakWithSmoke>false</breakWithSmoke>
                <canForceActive>false</canForceActive>
                <fireFoamBurster>false</fireFoamBurster>
                <versionInfo>Shining-Shield</versionInfo>
                <alias>闪灵的护盾</alias>
            </li>
            <li Class="AKE_VanyaShield.TCP_VanyaShieldExtension">
                <hideVanillaBubble>true</hideVanillaBubble>
                <bubbleStaticOverlay>
                    <texPath>Things/ShieldBelt/Medic/ShieldBeltShiningS</texPath>
                    <graphicClass>Graphic_Single</graphicClass>
                    <drawSize>(2,2)</drawSize>
                    <drawOffsetNorth>(0,0.1,0)</drawOffsetNorth>
                </bubbleStaticOverlay>
                <bubbleRotateTexPath>Things/ShieldBelt/Medic/ShieldBeltShiningD</bubbleRotateTexPath>
            </li>
        </comps>
    </ThingDef>

